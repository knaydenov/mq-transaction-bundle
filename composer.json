{
    "name": "kna/mq-transaction-bundle",
    "description": "This package provides transaction implementation for RabbitMQ using Doctrine",
    "type": "symfony-bundle",
    "autoload" : {
        "psr-4" : { "Kna\\MQTransactionBundle\\" : "" }
    },
    "autoload-dev": {
        "psr-4": {
            "Kna\\MQTransactionBundle\\Tests\\App\\": "Tests/App/src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Klim Naydenov",
            "email": "knaydenov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/orm": "^2.6",
        "symfony/console": "^4.2",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2",
        "symfony/monolog-bundle": "^3.3"
    }
}
